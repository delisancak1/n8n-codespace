# n8n basic auth ve sqlite örnek .env
# Kopyalayın: cp .env.example .env ve gerekli değerleri güncelleyin.

# n8n basic auth
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=changeme

# Kullanılacak n8n image (opsiyonel)
N8N_IMAGE=n8nio/n8n:latest

# SQLite (varsayılan, zaten docker-compose.yml içindeki yolu kullanır)
DB_TYPE=sqlite
DB_SQLITE_DATABASE=/home/node/.n8n/database.sqlite

# n8n host/port (opsiyonel)
N8N_HOST=0.0.0.0
N8N_PORT=5678

# Zaman dilimi (opsiyonel)
GENERIC_TIMEZONE=UTC